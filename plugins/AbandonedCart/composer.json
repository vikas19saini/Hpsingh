{
    "name": "lamppost/abandoned-cart",
    "description": "AbandonedCart plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "jeremy-dunn/php-fedex-api-wrapper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "AbandonedCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbandonedCart\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
